<? if self:get( 'services' ) then ?>
<table cellspacing="0" cellpadding="0">
	<thead><tr>
	<? for header, v in pairs( self:get( 'services' )[1] ) do ?>
		<th><?=header ?></th>
	<? end ?>
		<th></th>
	</tr></thead>

	<tbody>
	<? for k, service in pairs( self:get( 'services' ) ) do ?>
	<tr>
		<? for c, value in pairs( service ) do ?>
		<td><?=value ?></td>
		<? end ?>
		<td><a href="/service/<?=service.id ?>">View &rarr;</a></td>
	</tr>
	<? end ?>
	</tbody>
</table>
<? end ?>